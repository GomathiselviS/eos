                command += " {name} peer-filter {peer_filter}".format(**config_data['listen']['range']['peer_group'])
                command += " {name} remote-as {remote_as}".format(**config_data['listen']['range']['peer_group'])
    command = afi + " access-group {acl_name}".format(**config_data)
        command += " prefix-list {name} {direction}".format(**config_data['prefix_list'])
        command += " route-map {name} {direction}".format(**config_data['route_map'])
